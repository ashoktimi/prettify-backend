{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",

    "properties": {
        "product_key": {
            "type": "integer"
        },
        "brand_id": {
            "type": "integer"            
        },
        "name": {
            "type": "string"
        },
        "price": {
            "type": "integer"
        },
        "price_sign": {
            "type": "string"
        },
        "image_link": {
            "type": "string"
        },
        "product_link": {
            "type": "string",
            "default": ""
        },
        "website_link": {
            "type": "string",
            "default": ""
        },
        "description": {
            "type": "string"
        },
        "rating": {
            "type": "null",
            "default": null
        },
        "category_id": {
            "type": "null"
        },
        "type_id": {
            "type": "integer"
        },
        "created_at": {
            "type": "string",
            "default": ""
        },
        "updated_at": {
            "type": "string",
            "default": ""
        }
    },
    "additionalProperties": false,
    "required": [
        "product_key",
        "brand_id",
        "name",
        "price",
        "price_sign",
        "image_link",
        "description",
        "category_id",
        "type_id"
    ]
}